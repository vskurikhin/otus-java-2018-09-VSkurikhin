<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-ejb-jar PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 EJB 3.1//EN" 
    "http://glassfish.org/dtds/glassfish-ejb-jar_3_1-1.dtd">
<glassfish-ejb-jar>
    <enterprise-beans>
        <ejb>
            <ejb-name>DeptControllerBean</ejb-name>
            <ejb-ref>
                <ejb-ref-name>DeptController</ejb-ref-name>
                <jndi-name>java:global/HomeWork11Core/HomeWorkPersistent/DeptController</jndi-name>
            </ejb-ref>
        </ejb>
    </enterprise-beans>
    <keep-state>true</keep-state>
</glassfish-ejb-jar>
